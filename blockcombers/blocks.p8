function _draw_movie()
 state.m+=1
 local m=state.m
 local tx=(128-5*8)/2+16
 local ty=40
 
 --blocks
 
 --guy
 if(m<16)then
  local s=16
  if(m>8)s+=1
  spr(s,tx+2*8,ty+4*8)  
 elseif(m<22)then
  local o=max(0,(m-16)*2)
  if(m==16)sfx(0)
  spr(16,tx+2*8,ty+4*8-o)
 elseif(m<26)then
  local o=max(o,(m-20))+10
  spr(16,tx+2*8,ty+4*8-o)
 elseif(m<42)then
  local o=m-26
  local s=32
  if(m>32)s+=1
  spr(s,tx+2*8-o,ty+2*8) 
 elseif(m<50)then
  local o=min(m-(42),8)
  spr(16,tx,ty+2*8+o)
 elseif(m<64)then
  local o=min(m-50,8)
  local s=32  
  spr(32,tx+o,ty+3*8,1,1,true)
 elseif(m<72)then
  spr(16,tx+8,ty+3*8)
 elseif(m<80)then
  local o=m-72
  spr(16,tx+8,ty+3*8+o)
 elseif(m<96)then 
  local o=m-80
  local s=32
  if(m<88)s+=1
  spr(s,tx+8+o,ty+4*8,1,1,true)
 elseif(m<102)then
  if(m==100)sfx(2)
  spr(48,tx+3*8,ty+4*8)
 else
  spr(49,tx+3*8,ty+4*8)
 end  
 
 --_| 
 if(m<50)then
  spr(1,tx+8*1,ty+8*3)
  spr(2,tx    ,ty+8*4,2,1)
 elseif(m<72)then
  if(m==50)sfx(1)
  local ox=min(m-50+8*1,8*2)
  local oy=mid(8*3,m-58+8*3,8*4)
  spr(1,tx+ox ,ty+oy)
  spr(2,tx    ,ty+8*4,2,1)
 elseif(m==72)then
  sfx(4)
 end
 
 --z
 if(m<8*5)then
  local o=m-8
  spr(2,tx+3*8,ty+o  ,2,1)  
  spr(1,tx+3*8,ty+o-8)
  spr(2,tx+2*8,ty+o-16,2,1)
 elseif(m<72)then
  spr(2,tx+3*8,ty+8*4,2,1)
  spr(1,tx+3*8,ty+8*3)
  spr(2,tx+2*8,ty+8*2,2,1)
 elseif(m<80)then
  local o=m-72
  spr(1,tx+3*8,ty+8*3+o)
  spr(2,tx+2*8,ty+8*2+o,2,1)
 elseif(m<88)then
  spr(1,tx+3*8,ty+8*4)
  spr(2,tx+2*8,ty+8*3,2,1)
 elseif(m<96)then
  local o=m-88
  if(m==88)sfx(1)
  spr(1,tx+3*8+o,ty+8*4)
  spr(2,tx+2*8,ty+8*3,2,1)  
 else
  local o=min(m-96,8)
  spr(1,tx+4*8,ty+8*4)
  spr(2,tx+2*8,ty+8*3+o,2,1) 
 end  
 
 if(m>128+64)state.m=0

 --extent
 rectfill(tx,ty,tx+8*5,ty-8*5,1) 
 line(tx,ty+8*5,tx+8*5,ty+8*5,10)
end

