function _init_game()
 _init_screen()
	_init_blocks()
	_init_player()
	music(0)
end

function _update()
 if(state.s==0)then
  --title
  if(btnp()>0)then
   state.s=1
   _init_game()
  end
 elseif(state.s==1)then
	 _update_blocks()
	 _update_player()
	 _update_screen()
	 do_lines()
	elseif(state.s==2)then
	 --gameover
	 state.t+=1
	 if(state.t>8 and btnp()>0)then
	  state.s=1
	  state.t=0
	  _init_game()
	 end
	end
end

function _draw()
 cls(1)
	rect(0,0,127,127,10)
	if(state.s==1)then
	 _draw_player()
	end
	if(state.s>0)then
	 _draw_blocks()
	else
	 _draw_movie()
	 _draw_logo()	 
	end
	if(state.s==2)then
	 _draw_dead()
	end
	rectfill(0,0,127,8,1)
	rect(0,0,127,8,10)
 --score
 print("score:"..tostr(screen.l),2,2,10)
end 
