--screen

screen={l=0,t=0,d=false}

function _init_screen()
 screen={
	 x0=0,x1=128-8-1,
	 y0=0,y1=128-8-1,
	 t=0,i=0,--new spawn/block timers
	 q=nil,s=0,--spawn queue/col
	 tc=24,
	 l=0,--lines
	 d=false,
	}
end

function spawn_0_3(n)
 local q={}
 -- :- and vertical 2 variants
 if(n%2==0)then
  q[1]={1,0}
 else
  q[1]={0,1}
 end
 q[2]={2}
 if(n<2)then
  q[3]={1,0}
 else
  q[3]={0,1}
 end
 screen.q=q
 return 2
end

function spawn_4_6(n)
 if(n==4)then -- |
  screen.q={{1},{1},{1},{1}}
  return 1
 elseif(n==5)then -- _
  screen.q={{2,0,2}}
  return 4
 elseif(n==6)then -- []
  screen.q={{1,1},{2}}
  return 2
 end
end

function spawn_7_8(n)
 if(n==7)then--z
  screen.q={{2},{0,2}}
 elseif(n==8)then--s
  screen.q={{0,2},{2}}
 end
 return 3
end

function spawn_9_14(n)
 local l=((n-9)%3)+1
 screen.q={}
 for r=1,3 do--} and l
  if(r==l)then
   screen.q[r]={2}
  elseif(n<12)then
   screen.q[r]={1}
  else
   screen.q[r]={0,1}
  end
 end
 return 2
end

function spawn_15_20(n)
 screen.q={}
 local o={0,0,0}
 o[((n-15)%3)+1]=1
 local l={3}
 if(n<18)then-- _:_ and |__
  screen.q={o,l}
 else
  screen.q={l,o}
 end
 return 3
end

function spawn_block()
 --queue blocks
 local n=flr(rnd(21))
 local w=0
 if(n<4)then
  w=spawn_0_3(n)
 elseif(n<7)then
  w=spawn_4_6(n)
 elseif(n<9)then
  w=spawn_7_8(n)
 elseif(n<15)then
  w=spawn_9_14(n)
 else
  w=spawn_15_20(n)
 end

 --target player
 --local x=player.x+4-w/2
 --local c=(x-4)/8
 --if(c-flr(c)<0.5)then
 -- c=flr(c)
 --else
 -- c=flr(c)+1
 --end
 screen.s=flr(rnd(16-w))
end

function do_queue()
 if(screen.q==nil)return
 if(#screen.q==0)return
 local r=screen.q[#screen.q]
 if(r==nil)return
 del(screen.q,r)
 for o=1,#r do
  local w=r[o]
  if(w~=nil and w>0)then
   new_block(0,screen.s+o-1,w)
  end
 end
end

function _update_screen()
 --spawn timer
 screen.t-=1
 if(screen.t<=0)then
  spawn_block()
  screen.t=8*screen.tc
 end
 
 --block timer
 screen.i-=1
 if(screen.i<=0)then
  do_queue()
  screen.i=8
 end
 
 if(screen.d)then
  sfx(1)
  screen.d=false
 end
end
