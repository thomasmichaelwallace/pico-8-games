--block logic

function _init_blocks()
 blocks={}
 for i=0,15 do
  blocks[i]={}
 end
end

function _update_blocks()
 for i=15,0,-1 do for j=0,15 do
  if blocks[i][j]~=nil then
   blocks[i][j]:_update_y()
  end
 end end
 for i=15,0,-1 do
  local r={}--copy row
  for j=0,15 do
   r[j]=blocks[i][j]
  end
  for j=0,15 do
   if r[j]~=nil then
    r[j]:_update_x()
   end
  end
 end
end

function _draw_blocks()
 for i=0,15 do for j=0,15 do
  if blocks[i][j]~=nil then
   blocks[i][j]:_draw()
  end
 end end
end

function b_collide(s,b)
 if(s.y+8~=b.y)return false
 return
  (s.x>(b.x-s.w*8))and
  (s.x<(b.x+b.w*8))
end

function do_lines()
 local any=false
 for r=0,15 do
  local c=0
  local l=false
  while c<16 do
   local b=blocks[r][c]
   if(
    (b~=nil)and--exist
    (not b.f)--is not falling
   )then
    c+=b.w
    if(c>15)l=true
   else
    c=16--no line
   end
  end
  if(l)then
   any=true
   --destroy!
   for dc=0,15 do
    local b=blocks[r][dc]
    if(b~=nil)then
     blocks[r][dc]=nil
    end
   end
   --score
   screen.l+=1
  end
 end
 if(any)sfx(4)
end

function new_block(r,c,w)
 local b={
  r=r,c=c,--lhs r/c
  x=c*8,y=r*8,--tl x/y
  w=w,--width
  o=(w==3)and 4 or w,--sprite
  f=false,--true if just fell
  sl=0,sd=0,
  slide=function(s,dx)
   if(s.sl==0)then
    s.x+=dx--first move
	   s.sl=7
	   s.sd=dx
	  end
  end,
  _update_y=function(s)
   --base
   s.f=false
   if(s.y>screen.y1)return
   --block collide
   for c=0,15 do
    local b=blocks[s.r+1][c]
    if(b~=nil)then
     if(b_collide(s,b))return
    end
   end
   --move
   s.y+=1
   s.f=true
   --shift row
   local rn=s.y\8
   if(rn~=s.r)then
    blocks[s.r][s.c]=nil
    s.r=rn
    blocks[s.r][s.c]=s
   end
  end,
  _update_x=function(s)
   --slide
   if(s.sl>0)then
    s.x+=s.sd
    s.sl-=1
    local cn=s.x\8
    if(cn~=s.c)then
     if(blocks[s.r][s.c]==s)then
      blocks[s.r][s.c]=nil
     end
     s.c=cn
     blocks[s.r][s.c]=s
    end
   end
  end,
  _draw=function(s)
   spr(s.o,s.x,s.y,s.w,1)
   spr(s.o,s.x,s.y,s.w,1)
  end,
 }
 blocks[r][c]=b
 return b
end
