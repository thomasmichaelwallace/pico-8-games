player=nil

function add_all(t,vs)
 for v in all(vs) do
  add(t,v)
 end
end

function get_b_rhs(o)
 local bs={}--blocks effected
 local w=0--total width
 local c=o.c+o.w
 if(c==16)then
  --hit wall
  return{w=-1}
 end
 local rt=max(o.r-1,0)
 local rb=min(o.r+1,15)
 for r=rt,rb do
  local b=blocks[r][c]
  if(b~=nil)then
   if(
    (b.x==o.x+o.w*8)and--x
    (b.y+8>o.y)and--y/
    (b.y<o.y+8)
   )then
    add(bs,b)
    w+=b.w
    local h=get_b_rhs(b)
    if(h.w==-1)return{w=-1}--wall
    w+=h.w
    add_all(bs,h.bs)
   end
  end
 end
 return{bs=bs,w=w}
end

function get_b_lhs(o)
 local bs={}--blocks effected
 local w=0--total width
 local rt=max(o.r-1,0)
 local rb=min(o.r+1,15)
 local cl=max(o.c-3,0)
 local cr=max(o.c-1,0)
 if(o.x==0)return{w=-1}--wall
 for c=cl,cr do for r=rt,rb do
  local b=blocks[r][c]
  if(b~=nil)then
   if(
    (b.x+b.w*8==o.x)and
    (b.y+8>o.y)and--y/
    (b.y<o.y+8)
   )then
    if(b.x==0)return{w=-1}--wall
    add(bs,b)
    w+=b.w
    local h=get_b_lhs(b)
    if(h.w==-1)return{w=-1}--wall
    w+=h.w
    add_all(bs,h.bs)
   end
  end
 end end
 return{bs=bs,w=w}
end

function p_collide(p,dx)
 local o={
  x=p.x,y=p.y,
  r=p.y\8,c=p.x\8,
  w=1,
 }
 local h={w=0}
 if(dx>0)then--right
  h=get_b_rhs(o)
 else--left
  h=get_b_lhs(o)
 end
 if(h.w>0)then
  local m=false
  if(dx>0)then--left
   if(p.el>=h.w*p.ef)then
    m=true
    p.el=0
   else
    p.el+=1
   end
  elseif(dx<0)then--right
   if(p.er>=h.w*p.ef)then
    m=true
    p.er=0
   else
    p.er+=1
   end
  end
  if(m)then
	  p.x+=dx
	  for b in all(h.bs) do
	   b:slide(dx)
	  end
	  screen.d=true
	 end
 elseif(h.w==0)then
  p.x+=dx
  p.er=0
  p.el=0
 end
end

function can_fall(p,dy)
 local py=p.y+dy
 local rt=max(py\8,0)
 local rb=min(py\8+1,15)
 if(p.y\8==15)then
  p.y=120
  return false
 end
 local cl=max(p.x\8-3,0)
 local cr=min(p.x\8+1,15)
 for r=rt,rb do for c=cl,cr do
  local b=blocks[r][c]
  if(b~=nil)then
   if(
    (py+8>=b.y)and
    (py+8<=b.y+8)and
    (p.x>(b.x-8))and
    (p.x<(b.x+b.w*8))
   )then
    p.y=b.y-8
    return false
   end
  end
 end end
 p.y+=dy
 return true
end

function bump_head(p)
 --immedateily fall
 local rt=max(p.y\8-1,0)
 local rb=min(p.y\8,15)
 if(rb==0)return true--top
 local cl=max(p.x\8-3,0)
 local cr=min(p.x\8+1,15)
 for r=rt,rb do for c=cl,cr do
  local b=blocks[r][c]
  if(b~=nil)then
   if(
    (p.y<b.y+8)and
    (p.y+8>=b.y+8)and
    (p.x>(b.x-8))and
    (p.x<(b.x+b.w*8))
   )then
    return true
   end
  end
 end end
 return false
end

function _init_player()
 local p={
  x=60,y=120,--x,y position
  s=16,--sprite,n
  g=2,--gravity
  j=0,--jumping power
  u=false,--grounded
  d=false,--dead
  er=0,el=0,--effort
  ef=4,--effort multiplier
  df=1,--dead frames
  dl=8,--dead lives
  a=0,--animation timer
  _update=function(s)
   --move
   local dx=0
   if(btn(0))then dx=-1
   elseif(btn(1))then dx=1
   end
   if(dx==0)then
    s.s=16
   else
    s.s=32*sgn(dx)
   end
   p_collide(s,dx)
   --jump
   if(btn(2)and s.u)then
    s.j=30
    sfx(0)
   end
   if(s.j>0)then
    local dj=2
    if(s.j<12)dj=1
    if(s.j<6)dj=-1
    s.y-=dj
    s.j-=abs(dj)
    if(bump_head(s))then
     s.j=0
    end
   end
   --gravity
   local dy=(s.j<=0)and s.g or 0
   s.u=not can_fall(s,dy)
   --crushed
   if(bump_head(s))then
    s.df+=1
    if(s.df>s.dl)then
     sfx(2)
     s.d=true
     state.s=2
    end
   else
    s.df=0
   end
  end,
  _draw=function(s)
   local p=abs(s.s)
   s.a+=1
   if(s.a>16)s.a=0
   if(s.a>8)p+=1
   local f=(s.s>0)and true or false
   --override if dying
   if(s.df>0)then
    f=0
    p=48
    if(s.df>5)p+=1   
   end   
   spr(p,s.x,s.y,1,1,f)
  end,
 }
 player=p
end

function _draw_player()
 player:_draw()
end

function _update_player()
 player:_update()
end
