__lua__
-- sugarush
-- by thomas michael wallace

state=0--0:intro 1:game 2:end

lvl={
 w=0,--wait until next
 w_max=30,--max wait until next
 s=0,--score
 l=0,--level
 v=0,--fall-rate
}

ani={
 t=0,--count
 k=false,--tick
 r=6,--frame rate
}

you={
	x=0,--x position
	y=88,--/y
	v=0,--velocity
	s=1,--sprite
}


function set_level()
 local s=(lvl.s*1)--todo:balance
 --let level by score
 lvl.v=min(1+s/10,5)
 you.v=min(1+s/10,5)
 lvl.w_max=max(0,30-s)
 ani.r=4
 if(s>15)ani.r=5
 if(s>25)ani.r=4
 if(lvl.s%1==0)add_star(true)
end

function _init_game()
 --reset
 _init_stars()
 actors={}
 you.x=60
 lvl.s=0
 set_level()
 --start
 state=1
end

function _update_screen()
 if(btn(0)and btn(1))then
   _init_game()
 end
 update_text()
end

function _draw_screen()
 cls(0)
 if(state==0)then
  draw_text("sugarush",39,28)
  print("avoid veg 😐",40,44,8)
  print("eat candy ♥",40,64,11)
  local x=12
  for i=3,7,1 do
   x+=16
   spr(i,x,52)
   spr(i+16,x,72)
  end
  print("press ⬅️ and ➡️ to start",16,100,5)
 else
  draw_text("gameover",39,28)
  print("scored",52,40,5)
  local st=tostr(lvl.s)
  draw_text(st,(128-(#st+1)*5)/2-2,50)
  print("press ⬅️ and ➡️ to retry",16,100,5)
 end
end

function _update_game()
 --movement
 if(btn(0))you.x-=you.v
 if(btn(1))you.x+=you.v
 you.x=mid(0,you.x,120)
 --animation
 ani.t+=1
 ani.k=(ani.t>ani.r)
 if(ani.k)then
  you.s+=1
  if(you.s>2)you.s=1
  ani.t=0
 end 
 --actors
 for a in all(actors) do
  a:_update()
		if(a.y>you.y and a.y<(you.y+8))then
		 if(a.x>(you.x-8) and a.x<(you.x+8))then
		 	del(actors,a)
		 	if(a.f)then
		 	 state=2--todo:allow die
		 	else
		 	 lvl.s+=1
		 	 set_level()
		 	end
		 end
		end
 end
 --spawn
 lvl.w-=1
 if(lvl.w<0)then
  add_actor()
  lvl.w=5+flr(rnd(lvl.w_max))
 end
 --stars
 for s in all(stars) do
  s:_update()
 end
end

function _draw_game()
 cls()
 for s in all(stars) do
  s:_draw()
 end
 --player
 spr(you.s,you.x,you.y)
 --actors
 for a in all(actors) do
  a:_draw()
 end
 --score
 print("score:"..lvl.s,7)
end

function _update()
 if(state==1)then
  _update_game()
 else
  _update_screen()
 end
end

function _draw()
 if(state==1)then
  _draw_game()
 else
  _draw_screen()
 end
end
