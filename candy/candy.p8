--stars

stars={}
tail={5,1,2,13,8,14,9,10,11,12,6,7}

function add_star(zero_y)
 yo=flr(rnd(128+72))-36
 if(zero_y)yo=-36
 local s={
  x=flr(rnd(128)),--pos x/
  y=yo,--pos y/
  v=0.5+rnd(0.75),--vel/fact/
  d=0,--speed delta
  _update=function(self)
   self.d=lvl.v*self.v
   self.y+=self.d
   if(self.y>164)then
    self.x=flr(rnd(128))
    self.y=-36
   end
  end,
  _draw=function(self)
   local p=min(flr(lvl.s),3*#tail+1)
   local t=max(0,p-2*#tail)
   local d=mid(0,p-#tail,#tail)
   local l=0
   local c=0
   for o=0,p,1 do
    l+=1
    if(t>0)then
     if(l==3)then
      t-=1
      c+=1
      l=0
     end
    elseif(d>0)then
     if(l==2)then
      d-=1
      c+=1
      l=0
     end
    else
     c+=1
    end
    pset(self.x,self.y+o,tail[c])
   end
  end,
 }
 add(stars,s)
end

function _init_stars()
 stars={}
 for i=0,36,1 do
  add_star()
 end
end

-->8
text={
 o=0,--colour offset
 l=#tail,--colour inc.
 d=1,
}

function update_text()
 ani.t+=1
 ani.k=(ani.t>ani.r)
 if(ani.k)then
  text.o+=1
  if(text.o>text.l)text.o=1
  ani.t=0
  text.d+=1
  if(text.d>1)text.d=0
 end
end

function draw_text(t,tx,ty)
 local y=ty
 local x=tx
 local c=1+text.o
 for p=1,#t+1,1 do
  local s=sub(t,p,p)
  c+=1
  if(c>text.l)c=1
  x+=5
  print(s,x,y+(p+text.d)%2,tail[c])
 end
end
