--actors

actors={}

function add_actor()
 local t=flr(rnd(5))+1
 local f=(rnd(1)<0.5)

 local a={
 	x=flr(rnd(121)), --pos x/
 	y=-8,--/y
 	t=t,--type (1-5) as sprite no
 	f=f,--is fruit
 	s=t+(f and 2 or 18),--sprite
 	r=0,--rotation
 	c=0,--movement ctr
 	m=t,--movement ttype
 	_draw=function(self)
 	 local fx=(self.r>0 and self.r<3)
   local fy=(self.r>1)
 	 spr(self.s,self.x,self.y,1,1,fx,fy)
 	end,
 	_update=function(self)
 		self.y+=lvl.v
 		if(self.m==5)then
 		 self.m=flr(rnd(4))+1
 		end
 		if(self.m==2)then
    if(self.c==0)self.x+=lvl.v*0.5
    if(self.c==1)self.x-=lvl.v*0.5
    if(self.x>120)self.c=1
    if(self.x<0)self.c=0
   elseif(self.m==3)then
    if(self.c==0)self.c=self.x
    self.x=(sin(self.y/128))*(lvl.v*3)+self.c
   elseif(self.m==4)then
    self.c+=0.02
    if(self.c>1)self.c=0
    self.y-=(lvl.v*0.5)*sin(self.c)
   end
 		if(ani.k)self.r+=1
 		if(self.r>3)self.r=0
 		if(self.y>128)del(actors,self)
 	end
 }
 add(actors,a)
end
