--utilities

function pot_fill()
 local x=64
 local y=28
 local s=10
 
 y+=0.5*s
 
 rs={
  {3  ,1.5-0.5,7 },
  {2.5,3  -1.5,7 },
  {2  ,4  -3  ,3 },
  {2  ,4.5-4  ,10},
  {2  ,5.5-4.5,8 },
  {2  ,6  -5.5,10},
  {2  ,8  -6  ,3 },
 }
 for r in all(rs) do
  rectfill(x-r[1]*s,y,
   x+r[1]*s,y+r[2]*s-1,r[3])
  y+=r[2]*s
 end
end

function poly(ps,ox,oy,s)
 for i=2,#ps do
  local p0=ps[i-1]
  local p1=ps[i]
  
  local x0=ox+p0[1]*s
  local y0=oy+p0[2]*s
  local x1=ox+p1[1]*s
  local y1=oy+p1[2]*s
  
  line(x0,y0,x1,y1,p0[3])
 end
end

function flip_y(ps)
 local f={}
 for p in all(ps) do
  add(f,{-p[1],p[2],p[3]})
 end
 return f
end

function set_c(ps,c)
 local w={}
 for p in all(ps) do
  add(w,{p[1],p[2],c})
 end
 return w
end

function spr_rect(x0,y0,x1,y1,n)
 for x=x0,x1,8 do
	 for y=y0,y1,8 do
	  local rx=1
	  if((x+8)>x1)rx=(x1-x+1)/8
	  local ry=1
	  if((y+8)>y1)ry=(y1-y+1)/8
	  spr(n,x,y,rx,ry)	
	 end
 end
end

function bubble(f,x,y,cs,cf)
 for dx=-1,1 do
  for dy=-1,1 do
   f(x+dx,y+dy,cs)
  end
 end
 f(x,y,cf)
end
