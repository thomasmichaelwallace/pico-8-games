--pot

pot={}

function init_pot()
	pot={
	 water=0,
	 brew=0,
	 soy=0,
	 stock=12,
	 noodles=1,
	 swell=0,
	 lid=65,
	 broken=false,
	 pour=false,
	 sachet=false,
	 packet=60,
	 closed=false,
	 t=-1,
	 st=0,
	 cold=0,
	 eat=0,
	 e_max=60,
	 floor=false,
	}
end

function draw_pot()
 local x=64
 local y=28
 local s=10
 
 local wy=y+8*s
 wy-=min(7.5*s,pot.water)
 
 --streams
 px=x+1.5*s
 if pot.pour then
  line(px,20,px,y+0.5*s-1,12)
  if (not pot.closed) then
   line(px,y+0.5*s,px,wy,12)
  end
 end
 if pot.sachet then
  line(px,20,px,y+0.5*s-1,5)
  if (not pot.closed) then
   line(px,y+0.5*s,px,wy,5)
  end
 end
 
	--overflows 
 local ds={
  {1.5*s,0.5*s-1,12},
  {-3*s-1,0.5*s-1,12},
  {-3*s-1,10*s,12},
 }
 if pot.closed then
  if(pot.pour)then
   poly(ds,x,y-1,1)
   pot.floor=true
  end
  if pot.sachet then
   poly(set_c(ds,5),x,y-1,1)
   pot.floor=true
  end
 end
 if pot.water>=7.5*s and pot.pour then
  poly(ds,x,y-1,1)
  line(x+-3*s,y+0.5*s-1,
   x+1.5*s,y+0.5*s-1,12)
 end
  
 --soy
 local sx=pot.soy*2
 local sy=wy-1
 while sx>0 and sy>y  do
  local max_x=2*s
  if(sy<3*s+y)max_x+=0.5*s
  if(sy<1.5*s+y)max_x+=0.5*s
  local slx=min(sx,max_x)
  line(x-slx,sy,x+slx,sy,5)
  sy-=1
  sx-=max_x
 end
 
 --lid
 if(pot.lid>0 and not pot.broken)then
	 local lx=x-3*s+pot.lid
	 lx=mid(x-3*s-1,lx,x+3*s)
	 line(x-3*s,y+0.5*s-1,
	  lx,y+0.5*s-1,6)
 end
 
 --internal water
 if wy<y+1.5*s then
  rectfill(x-3*s,wy,
   x+3*s,y+1.5*s,pot.stock)
  wy=y+1.5*s
 end
 if wy<y+3*s then
  rectfill(x-2.5*s,wy,
   x+2.5*s,y+3*s,pot.stock)
  wy=y+3*s
 end
 wy=min(y+8*s,wy)
 rectfill(x-2*s,wy,
 	x+2*s,y+8*s,pot.stock)
  
 --noodles
 local ny=y+3.5*s
 local by=y+8*s-pot.brew
 local ns=pot.swell
 if(ny<by)then
  ns*=0.5
  spr_rect(x-2*s,by-ns,
  	x+2*s,y+8*s,pot.noodles)
  spr_rect(x-2*s,ny-ns,
   x+2*s,by-ns,1)
 else
  ny-=ns
  spr_rect(x-2*s,ny,
  	x+2*s,y+8*s,pot.noodles)
 end
 
 --pot
 local ps={
  {-3,0.5,7},
  {-3,1.5,7},{-2.5,1.5,7},
  {-2.5,3,7},{-2,3,3},
  {-2,4,8},
  {-2,4.5,10},{-2,5.5,8},
  {-2,6,3},{-2,8,3},
  {-2,8,7},{0,8,9},
 }
 poly(ps,x,y,s)
 poly(set_c(ps,7),x+1,y,s)
 poly(flip_y(ps),x,y,s)
 poly(set_c(flip_y(ps),7),x-1,y,s)

 --eat
 if(pot.eat>0)then
	 local rx=32
	 local ry=8
	 rectfill(rx,64-ry,128-rx,64+ry+2,0)
	 rect(rx+2,64-ry+2,128-rx-2,64+ry+2-2,7)
	 local str="mom mom mom?"
	 print(str,(128-#str*4)/2+1,64-5/2+1)
	 local rn=(128-2*rx-10)*min(1,pot.eat/pot.e_max)
	 line(rx+5,64+ry-3,rx+5+rn,64+ry-3,9)
	end
end

function update_pot()
 if btn(0) and pot.lid>0 then
  pot.lid-=1
  if(pot.lid<=0)pot.broken=true
 end
 if(btn(1) and pot.lid<60)pot.lid+=1
 pot.closed=pot.lid>45

 pot.sachet=false
 pot.pour=btn(3)
	if btn(2) and pot.packet>0 then
		pot.sachet=true
		pot.packet-=1
	end

 if not pot.closed then
  if(btn(2) and pot.packet>0)then
		 pot.soy+=1
		end
		if(btn(3) and pot.water<80)then
		 pot.pour=true
		 pot.water+=1
		 pot.cold-=2
		 if(pot.water>pot.brew)pot.brew=pot.water
		end
	end
	if pot.soy>0 and pot.water>0 then
	 pot.soy-=0.25
	 pot.st+=0.25
	 if(pot.st==1)pot.stock=15
		if(pot.st==25)pot.stock=4
		if(pot.st==45)pot.stock=5
	end
	
	if pot.water>5 and pot.swell<30 then
 local t=50
 if(pot.lid>50)t=10
	 pot.water-=1/t
	 pot.swell+=0.4/(2*t)
	 
	 if(pot.swell>3)pot.noodles=2
  if(pot.swell>12)pot.noodles=3
  if(pot.swell>25)pot.noodles=4
	end
	
	if btn(4)or btn(5) then
	 pot.eat+=1
	else
	 pot.eat=0
	 if(pot.brew>5)then
	  local c=pot.closed and 0.1 or 1
	  pot.cold+=c
	 end
	end
	
	if(pot.eat>pot.e_max)then
	 u=update_end
	 d=draw_end
	end
end
