function update_title()
 if btnp(4) or btnp(5) then
  d=draw_ins
  u=update_ins
 end
end

function draw_title()
 pot_fill()
 line(39,43,89,43,5)
 bubble(function(x,y,c)
  print("noodle pot",x,y,c)
  end,45,76,7,8)
 
 print("press ‚ùé to start",
  64-17*2,100,7)
end

function update_ins()
 if btnp(4) or btnp(5) then
  init_pot()
  d=draw_pot
  u=update_pot
 end
end

function draw_ins()
 pot_fill()
 bubble(function(x,y,c)
	 print("",x,y,c)
	 print("instructions")
	 print("")
	 print("1.‚¨ÖÔ∏è half pull back lid")
	 print("2.‚¨áÔ∏è fill to bottom line")
	 print("3.‚¨ÜÔ∏è add flavour to taste")
	 print("4.‚û°Ô∏è cover to retain heat")
	 print("5.-- wait for water to absorb")
	 print("6.‚¨ÖÔ∏è remove lid")
	 print("7.‚ùé hold to eat")
	 print("")
	 print("press ‚ùé to start")
	end,8,32,0,7)
end

score={}

function update_end()
 local s={}
 local c=score.c==nil and 0 or score.c
 if btnp(4) or btnp(5) then
  c+=1
  if(c>3)then
   d=draw_title
   u=update_title
  end
 end
 	
	if pot.noodles==2 then
	 add(s,{"‚ô• - perfect noodles!",true})
	elseif pot.noodles==1 then
	 add(s,{"üòê - the noodles are hard!",false})
	else
	 add(s,{"üòê - the noodles are stodge?!",false})
	end
	
	if pot.stock==5 then
	 add(s,{"üòê - too much flavor!",false})
	elseif pot.stock==4 then
	 add(s,{"‚ô• - just enough flavour.",true})
	else
	 add(s,{"üòê - can't taste any flavor?!",false})
	end
	
	if pot.floor then
  add(s,{"üòê - water on the floor!",false})
 else
  add(s,{"‚ô• - you didn't make a mess.",true})
 end
 
 if pot.cold>0 and pot.cold<120 then
  add(s,{"‚ô• - just warm enough",true})
 else
  add(s,{"üòê - they're freezing cold!",false})
 end
 
 if pot.water>30 then
  add(s,{"üòê - it's practically soup!",false})
 else
  add(s,{"‚ô• - not too wet.",true})
 end
	
	score={s=s,c=c}
end

function draw_end()
 print("the scores are in...",0,32,7)
 print("")
 print("")
 local t=0
 for s in all(score.s) do
  if s[2] then
   t+=1
   color(11)
  else
   color(8)
  end
  print(s[1])
 end
 color(7)
 print("")
 print("you made a "..t.." ‚òÖ pot!")
 print("")
 color(5)
 print("hold ‚ùé to restart")
end
