function _init_car(t,x,y)
 local m=md[t]
 local car={
  x=x,
  y=y,
  m=m,
  c=m.c,
  _draw=function(self)
  	spr(m.s,self.x,self.y,m.sx,m.sy)
  end,
  _move=function(self,dx,dy)
  	--try move x
  	self.x+=dx
  	if(testc(self))then
  	 self.x-=dx
  	end
  	self.y+=dy
  	if(testc(self))then
  		self.y-=dy
  	elseif(self.y==zn.e.b and
  	 dy==-1)then
  	  --do not renter loading
  	 self.y-=dy
  	end
  end
 }
 add(cs,car)
 add(ac,car)--add to actors
 return car
end

function _update_car()
	local dx,dy=0,0
	if(btn(0))then
		dx=-1
	elseif(btn(1))then
	 dx=1
	end
	if(btn(2))then
	 dy=-1
	elseif(btn(3))then
	 dy=1
	end
	if(pl~=nil)then
	 pl:_move(dx,dy)
	end
end
