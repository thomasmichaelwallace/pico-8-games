-- util

--return true if x,y is inside
-- r{x0,y0,x1,y1}
function pin(r,p)
 local x0,y0,x1,y1=unpack(r)
 local x,y=unpack(p)
 return (x>=x0 and x<=x1) and
 	(y>=y0 and y<=y1)
end

--returns true if
-- a{x0,y0,x1,y1} is in b{...}
function rcol(a,b)
	local a0={a[1],a[2]}
	local a1={a[3],a[4]}
	--reverse b
	local b0={b[3],b[2]}
	local b1={b[1],b[4]}
	return pin(a,b0) or
		pin(a,b1) or pin(b,a0) or
		pin(b,a1)
end

function roff(r,x,y)
 return {r[1]+x,r[2]+y,r[3]+x,r[4]+y}
end

-- returns true if 
--  a{x=,y=,c={{x0,y0,x1,y1},...}}
--  collides with b{...}
function ccol(a,b)
	for ra in all(a.c) do
	 for rb in all(b.c) do
	  if(rcol(roff(
	   ra,a.x,a.y),roff(
	   rb,b.x,b.y)))then
	    return true
	  end
	 end
	end
	return false
end

function testc(me)
 for a in all(ac) do
  if(a~=me)then
  	if(ccol(me,a))then
  	 return true
  	end
  end
 end
 return false
end

function sprc(s,x0,y0,x1,y1)
 local ox=(s%16)*8
 local oy=flr(s/16)*8
 return {x0-ox,y0-oy,x1-ox,y1-oy}
end

function _printc(str, y, c)
 print(str, 64 - (#str * 2), y, c) 
end
