-- zones

zn={}

function _init_zones()
 zn={
  c=nil,
  e={t=17,l=27,b=17+2*8},
  t=0,--time in
  u=0,--time out
  w=240,--wait
  nw=100,--min_wait
  xw=350,--max_wait
  v=40,--wait out
  r={t=95},
  a=nil,
  ai=nil,--ai index
  o=nil,
  s=0,
  h=1,--chance of winning
  l=-1
 }
end

function _draw_zones()
 --zones
 spr(11,zn.e.l,zn.e.t,3,2)
 spr(43,83,zn.r.t,3,2)
 --waiter
 if(zn.a~=nil)then
  spr(3,70,100)
	 spr(zn.a.s,65-zn.a.sx*8,104-zn.a.sy*4,zn.a.sx,zn.a.sy)
	 print("=",67,101,10)
	end
 --in car
 if(zn.c~=nil)then
  local c=zn.c
  local m=c.m
  spr(m.s,c.x,c.y,m.sx,m.sy)
 end
 --out car
 if(zn.o)then
  zn.o:_draw()
 end
 --timer
 local ry=10
 local rl=27
 local rw=23*(zn.t/zn.xw)
 if(rw>1)then
  line(rl,ry+1,rl+rw,ry+1,11)
  rect(rl,ry,rl+rw,ry+2,3)
 end
end

function _update_zones()
 --incar
 if(zn.c==nil)then
  if(zn.t>0)then
   zn.t-=1
  else
   -- +1 chance of matching.
   local i=flr(rnd(#md))+1+zn.h
   if(i==zn.l and rnd(10)>3)then
    i+=1
    if(i>#md)then
     i=1
    end
   end
   if(i>#md)then
    if(zn.ai~=nil)then
     i=zn.ai
    else
     i=flr(rnd(#md))+1
    end
   end
   local m=md[i]
   zn.l=i
   local x=m.sx*(-8)
   local y=zn.e.t+8-(m.sy*4)
   zn.c={x=x,y=y,m=m,i=i}
  end
 else
  if(zn.c.x<zn.e.l)then
   zn.c.x+=1
  else
   zn.w=zn.nw+flr(rnd(zn.xw-zn.nw))
   zn.t=zn.w--force wait
   local cr=_init_car(zn.c.i,zn.c.x,zn.c.y)
   local lx=cr.x
   cr:_move(1,0)
   if(lx==cr.x)then
    --game over!
    ui.l=true --end!
   end
   zn.c=nil
  end
 end
 --out car
 if(zn.a==nil and zn.o==nil)then
  if(zn.u>0)then
   zn.u-=1
  else
   zn.ai=flr(rnd(#md))+1
   zn.a=md[zn.ai]
  end
 end
 if(pl~=nil and zn.a~=nil)then
  if(pl.y>zn.r.t)then
   if(pl.m==zn.a)then
    zn.o=pl
    del(cs,pl)
    del(ac,pl)
    pl=nil
    zn.a=nil
    zn.s+=1
    if(zn.s>10)zn.h=0
   end
  end
 end
 if(zn.o)then
  zn.o.x+=1
  if(zn.o.x>127)then
   zn.o=nil
   zn.u=zn.v
  end
 end
end
