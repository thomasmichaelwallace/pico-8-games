 ac={}
 cs={}
	for x=0,63,1 do
		for y=0,63,1 do
			local t=mget(x,y)
			local c=bl[t]
			if(c~=nil)then
			 add(ac,{
			  x=x*8,y=y*8,s=t,c=c,
			  _draw=function(self)
			   spr(self.s,self.x,self.y)
			  end
			 })
			end
		end
	end
	
	add(ac,a)
end
