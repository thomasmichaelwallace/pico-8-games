ui={
 s=true,--show start
 l=true,--locked
}

function _update_ui()
 if(ui.l)then
  if(btnp(4) or btnp(5))then
   ui.l=false--unlock
   ui.s=false--never start
   _init()
  end
 end
end

function _draw_ui()
 print("score "..tostr(zn.s),
  0,0,7)
  
 if(ui.l and ui.s)then
  local w=100
  local h=6+(8*2+10*2)+2
  local x=(127-w)/2
  local y=(127-h)/2 
  rectfill(x,y,x+w,y+h,0)
  rect(x+1,y+1,x+w-1,y+h-1,7)
  y+=6
  _printc("parker",y,10)
  y+=1
  _printc("parker",y,7)
  y+=1
  rectfill(x+5,y,x+w-5,y+3,0)
  _printc("parker",y,12)
  x+=18
  y+=12
  spr(20,x,y,2,1)
  x+=18
  spr(37,x,y-4,2,2)
  x+=14
  spr(35,x,y)
  x+=10
  spr(36,x,y-4,1,2)
  x+=10
  spr(18,x,y)
  y+=15
  _printc("press ❎ to restart ",y,5) 
 elseif(ui.l)then
  local w=100
  local h=6+(8*2+10*2)+2
  local x=(127-w)/2
  local y=(127-h)/2 
  rectfill(x,y,x+w,y+h,0)
  rect(x+1,y+1,x+w-1,y+h-1,7)  
  y+=6
  _printc("gameover",y,7)
  y+=8
  _printc("you scored",y,7)
  y+=10
  _printc(tostr(zn.s),y,12)
  y+=10
  _printc("press ❎ to restart ",y,5) 
 end
end
