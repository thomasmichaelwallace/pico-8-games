-- mouse

local ms={
 x=0,
 y=0,
 c={{2,0,3,1}},
 k=false,--true on click
 b=0,
 s=2,
 i=false,
}

function _init_mouse()
 if(ms.i==false)then
  poke(0x5f2d, 1)
  ms.i=true
 end
end

function _update_mouse()
 ms.x=stat(32)-1
 ms.y=stat(33)-1
 --0,1,2,4
 --none,left,right,middle
 local b=stat(34)
 --detect click
 if(ms.b==0 and b==1)then
  ms.k=true
 else
  ms.k=false
 end
 ms.b=b
 --assign
 if(ms.k==true)then
  for c in all(cs) do
  	if(ccol(c,ms))then
  	 pl=c
  	end
  end
 end
end

function _draw_mouse()
 spr(ms.s,ms.x,ms.y)
end
