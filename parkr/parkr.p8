function _init()
 _init_mouse()
 _init_zones()
	_init_actors()
end

function _update()
 _update_mouse()
 if(ui.l)then
  _update_ui()
 else
  _update_zones()
  _update_car()
 end
end

function _draw()
	cls()
	_draw_zones()
	for a in all(ac) do
		a:_draw()
	end
	_draw_mouse()
	_draw_ui()
end

