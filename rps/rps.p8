mov={}
--s:scenes {d:draw,n:for ticks}
--cb:callback when over
function init_movie(s,cb)
 mov.s=s--scenes:{d,n}
 mov.n=1--scene number
 mov.f=0--frame number
 mov.t=0--frame timer
 mov.cb=cb
 gme.m=1
end
init_movie()

function update_movie()
 if(btnp(5))then--skip
  mov.t=0
  mov.f=mov.s[mov.n].n
 end
 mov.t+=1
 if(mov.t==12)then--fps
  mov.f+=1
  mov.t=0
 end
 if(mov.f==mov.s[mov.n].n)then
  mov.n+=1
  mov.f=0
  if(mov.n>#mov.s)then--end
   mov.cb()
  end
 end
end

function draw_movie()
 cls()
 mov.s[mov.n].d()
 if(not gme.d)then
  text_rect("hold ❎ to fast forward",0,0,127,127,1,1,5)
 end
end

