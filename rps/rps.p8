function draw_hall(h)
 --left person  
 if(h.rl==2)then
  --rise
  sprp(1,43,46,1,2,1)
 elseif(h.rl==3)then
  --die
  sprp(49,33,55,2,1,1)
 elseif(h.rl==4)then
  --fall
  sprp(49,32,56,2,1,1)
 else
  --normal
  sprp(1,44,48,1,2,1)
 end
 
 --right person
 if(h.rr==2)then
  sprp(4,77,46,1,2,2)
 elseif(h.rr==3)then
  sprp(51,79,55,2,1,2)
 elseif(h.rr==4)then
  sprp(51,80,56,2,1,2)
 else
  sprp(4,76,48,1,2,2)
 end
 
 --official
 spr(2,60,44,1,3)
 if(h.gl)spr(3,53,49,1,1)
 if(h.gr)spr(19,66,49,1,1)
 if(h.bl)spr(35,52,48,1,1,true)
 if(h.br)spr(35,66,47,1,1)
end

function build_hall(l,r)
 if(l==nil and r==nil)then
  return {{
   d=function()draw_hall({})end,
   n=3,
  }}
 end
 local s={
  {d=function() draw_hall({
   })end,
   n=3},
  {d=function() draw_hall({
    gl=l,gr=r,
   })end,
   n=1},
  {d=function() draw_hall({
    gl=l,gr=r,bl=l,br=r,
   })end,
   n=1},
 }
 local rls={1,1,1}
 local rrs={1,1,1}
 local ns={1,1,7}
 if(l)rls={2,3,4}
 if(r)rrs={2,3,4}
 for i=1,3 do
  local n=1
  if(i==3)n=5
  add(s,{
   d=function() draw_hall({
    gl=l,gr=r,rl=rls[i],rr=rrs[i],
   })end,
   n=ns[i],
  })
 end
 return s
end
