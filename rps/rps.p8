gme={
 m=0,--mode
 d=false,--debug/ss mode
 s=false,--ss mode
}

function _init()
 init_modes()
	start_intro()
end

modes={}
function init_modes()
 modes={
  {u=update_movie,d=draw_movie},
  {u=update_read,d=draw_read},
  {u=update_level,d=draw_level},
  {u=update_lose,d=draw_lose},
  {u=update_win,d=draw_win},
 }
end

function _update()
 if(gme.d)then
  if(btnp(2))gme.s=true
  if(btnp(3))gme.s=false
 end
 if(not gme.s)then
  local m=gme.m
  modes[m].u()
 end
end
function _draw()
 cls()
 local m=gme.m
 modes[m].d()
end
