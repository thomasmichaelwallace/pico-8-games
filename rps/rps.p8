--m3:level

lvl={
 n=1,--level no.
 s=2,--1-3/rps
 o=nil,--current opponent,
 l={3,5,6},--qtr/semi/final opps
}

pal_p={--player pallet
 c={4,9,12,10},
 b=false,
}

function gen_levels()
 local a={}
 for i=2,#ppl do
  add(a,i)
 end
 lvl.l={}
 while(#lvl.l<3)do
  local n=flr(rnd(#a))+1
  if(a[n]~=nil)then
   add(lvl.l,a[n])
   a[n]=nil
  end
 end
end

function start_level(n)
 lvl.n=n
 if(lvl.n==1)gen_levels()
   
 lvl.o=ppl[lvl.l[lvl.n]]
 
 pals[1]=pal_p
 pals[2]=lvl.o.p
 
 local s={}
 add_all(s,build_hall())
 local t="the quarter final"
 if(lvl.n==2)t="the semi finals"
 if(lvl.n==3)t="the final round"
 add(s,{
  d=function()
   text_rect(t,
    0,0,127,127,0,0,lvl.o.p.c[2])
  end,
  n=3,
 })
 init_movie(s,function()
  gme.m=3--start level loop
 end)
end

function end_level()
 local l=false--left dies
 local r=false--right dies
 local d=false--draw (both die)
 local wm={2,3,1}
 local c=lvl.o.c
 if(lvl.s==c)then
  d=true
 elseif(lvl.s==wm[c])then
  r=true
 else
  l=true
 end
 local m={}
 add_all(m,build_battle(lvl.s,c))
 add_all(m,build_hall(l or d,r or d))
 if(r and lvl.n<3)then
 	add(m,{
	  d=function()
	   text_rect(
	    "you survived for another round"
	    ,0,0,127,127,0,0,7)
	  end,
	  n=3
	 })
	end
	local cb=function()
   gme.m=4
 end
 if(r)then
  if(lvl.n<3)then
	  cb=function()
	   start_level(lvl.n+1)
	  end
	 else
	  cb=function()
	   gme.m=5--win!
	  end
	 end
 end
 init_movie(m,cb)
end

function update_level()
 if(btnp(4))then
  end_level()
  return
 end
 if(btnp(0))lvl.s-=1
 if(btnp(1))lvl.s+=1
 lvl.s=mid(1,lvl.s,3)
end

