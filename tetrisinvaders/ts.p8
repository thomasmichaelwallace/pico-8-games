--stars

st={}

function _init_st()
 local c={13,6,7}
 st={}
 for i=1,32 do
  local s=rndi(1,3)
  add(st,{
   x=rnd(128),
   y=rnd(128),
   c=c[s],
   s=s,
  })
 end
end

function _update_st()
 for s in all(st) do
  s.x-=s.s
  if(s.x<0)then
   s.x=128
   s.y=rndi(0,127)
  end
 end
end

function _draw_st()
 for s in all(st) do
  pset(s.x,s.y,s.c)
 end
end
