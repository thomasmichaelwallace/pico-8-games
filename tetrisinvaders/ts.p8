--tetris

ts={}
function _init_ts()
 ts={}
 add_tss()
end

function del_ts(t)
 if(t.l)then
  t.l.a=false
 end
 for l in all(ts) do
  if(l.l~=nil and l.l==t)l.l=nil
 end
 del(ts,t)
end

pals={--d/l
 { 5, 6},--gray
 { 8, 9},--red
 { 3,11},--green
 { 9,10},--yellow
 { 1,12},--blue
 {13,14},--pink
 { 6, 7},--white
}

--r:row,c:column offset
function add_ts(r,c,p)
 local t={
  x=128+c*8,y=r*8,--t/l x/y
  s=1,--sprite
  l=nil,a=false,--link/attached
  h=30,--health
  m=false,--moved
  p=p,
  _draw=function(s)
   pal(5,s.p[1])
   pal(6,s.p[2])
   spr(s.s,s.x,s.y)
   pal()
  end,
  do_move=function(s) 
   local m=true--move
   for t in all(ts) do
    if(t.y==s.y)then
     if(t.x+8==s.x)m=false
    end
   end
   if(m and s.l)m=s.l:do_move()
   if(not m)return false
   s.x-=1*gm.z
   return true
  end,
  _update=function(s)
   if(s.a)return--attached
   if(s.x==0)return--base
   s.m=s:do_move()
  end,
 }
 add(ts,t)
 return t
end

shapes={
 --1 no.orientaitons (1)
	{{1,1},--o
	 {1,1}},
	--2 n/o (2-3)
	{{1,1,1,1}},--i
	{{1,1,0},--z
	 {0,1,1}},
	{{0,1,1},--s
	 {1,1,0}},
	--4 n/o (4-6)
	{{1,0},--l
	 {1,0},
	 {1,1}},
	{{0,1},--j
	 {0,1},
	 {1,1}},
	{{0,1,0},--t
	 {1,1,1}},
}

function add_tss()
 local n=rndi(1,7)
 local s=shapes[n]
 local o=rndi(0,3)
 s=rotatemt(s,o)
 local ro=rndi(0,16-#s[1])
 local l=pals[n]
 --preserve ref. order
 for c=1,#s do
  local p=nil --past row
  for r=1,#s[c] do
   if(s[c][r]==1)then
    local a=add_ts(ro+r-1,c-1,l)
    if(p~=nil)then
     p.l=a
     a.a=true
    end
    p=a
   else
    p=nil
   end
  end
 end
end

function _draw_ts()
 for t in all(ts) do
  t:_draw()
 end
end

function _update_ts()
 for t in all(ts) do
  t:_update()
 end
 --test lines
 local l={}
 local n=true--new
 local z=1--lines cleared
 for t in all(ts) do
  if(t.x%8==0 and
   (t.x==0 or t.a or (not t.m))
  )then--only full line
   local c=t.x/8+1
   --detect gameover
   if(c==17 and not t.m and not t.a)then
    gm.o=true
    gm.t=true
   end
   
   if(l[c]==nil)l[c]={}
   add(l[c],t)
   if(#l[c]==16)then--full line
    for f in all(l[c]) do
     del_ts(f)
    end
    gm.s+=500*z
    z+=1--up the factor
   end
  else
   n=false
  end
 end
 if(n)add_tss()
end
