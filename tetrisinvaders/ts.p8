--bullets

bl={}
function _init_bl()
 bl={}
end

function add_bl()
 local b={
  x=pl.x+8-1,y=pl.y+4-1,
  _draw=function(s)
   rect(s.x,s.y,s.x+1,s.y+1,8)
  end,
  _update=function(s)
   s.x+=2
   if(s.x>128)del(bl,s)
   local mx,my=s.x,(s.y\8)*8
   for t in all(ts) do
    if(t.y==my)then
     if(mx>t.x and mx<t.x+8)then
      del_ts(t)
      del(bl,s)
      gm.s+=1
     end
    end
   end
  end
 }
 add(bl,b)
end

function _update_bl()
 if(btnp(4)or btnp(5))then
  add_bl()
 end
 for b in all(bl) do
  b:_update()
 end
end

function _draw_bl()
 for b in all(bl) do
  b:_draw()
 end
end
