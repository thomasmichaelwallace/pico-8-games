--tetris-invaders.
--by thomas michael wallace

gm={}--game state
function _init()
 gm={
  t=true,--title
  s=0,--score
  o=false,--over
  f=0,--frame
  z=1,--speed *must make 8*
 }
 _init_st()
 _init_pl()
 _init_ts()
 _init_bl()
end

function _update_gm()
 gm.f+=1
 if(gm.f>32)gm.f=0
 if(btn(0)and btn(1))then
  if(gm.o)then
   _init()--reset
  else
   gm.t=false--start
  end
 end
end

function _update()
 _update_st()
 if(gm.t)then
  _update_gm()
 else
	 _update_ts()
	 _update_bl()
	 _update_pl()
	end
end

function _draw_gm()
 --title screen
 if(gm.o)then
  --gameover
  local y=40
  local w,h=80/2,52/2
  rectfill(63-w,63-h,63+w,63+h,0)
  w-=1
  h-=1
  rect(63-w,63-h,63+w,63+h,7)
  y+=3
  printc("~ game over ~",y,8)
  y+=12
  printc("you scored",y,6)
  y+=8
  printc(tostr(gm.s),y,12)
  y+=12
 	printc("press ⬅️ and ➡️  ",y,5)
 	y+=6
 	printc("to try again",y,5)
 else
	 local x=(128-#"tetrisinvaders"*4)/2
	 local d=lnsin(gm.f/32)*6
	 local y=48
	 print("tetris",x,y-d,11)
	 x+=#"tetris"*4
	 print("invaders",x+d,y,10)
	 y+=28
	 printc("press ⬅️ and ➡️ to start  ",y,5)
 end
end

function _draw()
 cls()
 _draw_st()
 _draw_bl()
 _draw_pl()
 _draw_ts()
 --score
 local x=128-#(tostr(gm.s))*4
 print(gm.s,x,0,7)
 --screens
 if(gm.t)then
  _draw_gm()
 end
end

