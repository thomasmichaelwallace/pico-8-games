--utilities

--random int
--l:lowest/h:highest inc.
function rndi(l,h)
 return l+flr(rnd((h-l)+1))
end

function lnsin(r)
 local f=r
 if(f>=0.5)f=1-f
 return f
end

function printc(t,y,c)
 local x=(128-#t*4)/2
 print(t,x,y,c)
end

function rotatem(m)
 local h,w=#m,#m[1]
 local t={}
 for r=1,h do
  for c=1,w do
   if(t[c]==nil)t[c]={}
   t[c][h-r+1]=m[r][c]
  end
 end
 return t
end

function rotatemt(m,t)
 local r=m
 for i=1,t do
  r=rotatem(r)
 end 
 return r
end
