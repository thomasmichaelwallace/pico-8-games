--player

pl={}--player state

function _init_pl()
 pl.x,pl.y=59,59--t/l x/y position
 pl.a=0--sprite ani. frame
 pl.d=0--movement dir(0⬅️➡️⬆️⬇️3)
 pl.s=2--speed
end

function _draw_pl()
 local s=16+pl.d
 if(pl.a>8)s+=16
 spr(s,pl.x,pl.y)
end

function _update_pl()
 pl.a+=1
 if(pl.a>16)pl.a=0
 
 local dx,dy=0,0
 local d=-1
 if(btn(0))then
  d,dx=0,-1*pl.s
 elseif(btn(1))then
  d,dx=1, 1*pl.s
 end
 if(btn(2))then
  d,dy=2,-1*pl.s
 elseif(btn(3))then
  d,dy=3, 1*pl.s
 end
 pl.d=(d==-1 and 1 or d)
 
 local x=mid(0,pl.x+dx,120)
 local y=mid(0,pl.y+dy,120)
 
 for t in all(ts) do
  if(y+8>t.y and y<t.y+8)then
   if(x+8>t.x and x<t.x+8)then
    if(d~=1)then
     x,y=pl.x,pl.y
    else
     y=pl.y
    end
   end
  end
 end
 
 pl.x,pl.y=x,y
end
